  GROUP gewaesser
  TYPE LINE
  STATUS [% ON_GEWAESSER %]
  DATA wasserstrassen
  
  LABELITEM "NAME"

  CLASSITEM 'CAT'

[% BLOCK CLASS_W_ANY -%]
  CLASS
    EXPRESSION "[% expr %]"
    LABEL
      MINFEATURESIZE [% IF expr == "W" || expr == "W0" %]200[% ELSE %]40[% END %]
      MINDISTANCE 150
      POSITION auto
      TYPE truetype
      FONT "lucida"
      MINSIZE 8
      COLOR 0 0 255
      ANGLE AUTO
      PARTIALS FALSE
    END
    SYMBOL "circle"
    COLOR 180 180 255
    SIZE [% size %]
    [% IF maxscale %]MAXSCALE [% maxscale %][% END %]
    [% IF minscale %]MINSCALE [% minscale %][% END %]
  END
[% END -%]
[% PROCESS CLASS_W_ANY expr="W" size=1 minscale=20000 maxscale=400000 -%]
[% PROCESS CLASS_W_ANY expr="W" size=2 minscale=10000 maxscale=20000 -%]
[% PROCESS CLASS_W_ANY expr="W" size=4 minscale=undef maxscale=10000 -%]

